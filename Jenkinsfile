pipeline {
	agent any
		stages {
			stage('First') {
				steps {
					sh '''
						echo "Step First"
					'''
				}
			}


			stage('Second') {
				steps {
					sh '''
						echo "Step Second"
					'''
				}
			} 

			stage('Third') {
				steps {
					sh '''
						echo "Step Third"
					'''
				}
			}
		}
}



